@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Registration</title>
    <link href="../Content/Site.css" rel="stylesheet" />
</head>
<body>
    <div>
        <p>Name: @ViewBag.Name</p>
        <p>Age: @ViewBag.Age</p>
        <p>Gender: @ViewBag.Gender</p>
        <p>Email: @ViewBag.Email</p>
        <p>Telephone: @ViewBag.Telephone</p>
        <p>Note via: @ViewBag.Notification</p>
        <p>Password: @ViewBag.Password</p>
        <p>Remember account: @ViewBag.Remeber</p>
        
        <form role="form" action="/Home/HomePage">
            <button type="submit" class="btn btn-default">To home page</button>
        </form>
    </div>
</body>
</html>
